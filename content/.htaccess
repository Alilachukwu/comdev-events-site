RewriteEngine On

# Redirect to https is done elsewhere

# ApacheCon lives on its own domain
Redirect temp /apachecon https://www.apachecon.com

# Redirect to current CoC event website
# This should agree with
# https://raw.githubusercontent.com/apache/www-site/main/content/events/.htaccess
#  and the default event in
# https://www.apachecon.com/event-images/snippet.js
Redirect /x/current-event.html https://communityovercode.org/
